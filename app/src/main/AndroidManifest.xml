<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.wilderossi.blupresence">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:debuggable="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".InstituicaoListActivity"      />
        <activity android:name=".InstituicaoFormActivity"      />
        <activity android:name=".AuthenticationFormActivity"   />
        <activity android:name=".TurmaInstituicaoListActivity" />
        <activity android:name=".TurmaListActivity" />

    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>